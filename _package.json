{
  "name": "vscode-fish-lsp",
  "displayName": "Fish Language Support",
  "description": "Fish shell language support via fish-lsp",
  "version": "0.1.0",
  "publisher": "ndonfris",
  "homepage": "https://fish-lsp.dev",
  "repository": "https://github.com/ndonfris/fish-lsp",
  "bugs": {
    "url": "https://github.com/ndonfris/fish-lsp/issues"
  },
  "icon": "images/fish-lsp-logo.png",
  "galleryBanner": {
    "color": "#181939",
    "theme": "dark"
  },
  "author": "ndonfris",
  "license": "MIT",
  "keywords": [
    "fish",
    "shell",
    "lsp",
    "language-server",
    "fish-shell",
    "intellisense",
    "code-completion",
    "language-support"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:fish"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "fish",
        "aliases": [
          "Fish",
          "fish"
        ],
        "extensions": [
          ".fish"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Fish Language Server",
      "properties": {
        "fish-lsp.trace.server": {
          "type": "string",
          "scope": "window",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "fish-lsp": "^1.0.8",
    "vscode-languageclient": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  }
}
