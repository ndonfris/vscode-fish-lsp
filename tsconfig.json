{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "out",
    "rootDir": "src",
    "strict": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "noUnusedParameters": true
  },
  "exclude": ["node_modules", ".vscode-test", "out"],
  "include": ["src"]
}
